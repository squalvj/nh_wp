.bg,.bg-gradient-left-right
	position absolute
	top 0
	left 0
	right 0
	bottom 0
	z-index 1
	img
		height 100%
		width 100%
		object-fit cover
		object-position top center
		position absolute
	&:after
		position absolute
		content ''
		width 100%
		height 100%
		top 0
		left 0
		background linear-gradient(rgba(0,0,0,0.6), transparent)
.bg-gradient-left-right
	&:after
		position absolute
		content ''
		width 100%
		height 100%
		top 0
		left 0
		background: rgba(0,0,0,0.2); /* For browsers that do not support gradients */    
		background: -webkit-linear-gradient(left, rgba(0,0,0,0.6), transparent); /* For Safari 5.1 to 6.0 */
		background: -o-linear-gradient(right, rgba(0,0,0,0.6), transparent); /* For Opera 11.1 to 12.0 */
		background: -moz-linear-gradient(right, rgba(0,0,0,0.6), transparent); /* For Firefox 3.6 to 15 */
		background: linear-gradient(to right, rgba(0,0,0,0.6), transparent); /* Standard syntax (must be last) */
	

