$black = #1D1D1D

button:focus
	outline none !important

display()
	-webkit-display arguments
	-moz-display arguments
	-ms-display arguments
	-o-display arguments
	display arguments
flex-direction()
	-webkit-flex-direction arguments
	-moz-flex-direction arguments
	-ms-flex-direction arguments
	-o-flex-direction arguments
	flex-direction arguments
transition()
	-webkit-transition: arguments
	-moz-transition: arguments
	-ms-transition: arguments
	-o-transition: arguments
	transition: arguments

.rel
	position relative
h1,h2,h3,h4,h5
	font-family openR
	color white
body
	//-webkit-font-smoothing: antialiased;
	text-rendering: optimizeLegibility;
	font-feature-settings: "kern";
	-webkit-font-feature-settings: "kern";
	-moz-font-feature-settings: "kern";
	-moz-font-feature-settings: "kern=1";
	font-kerning: normal;	
	overflow-x hidden
	

#myVideo
	position: absolute;
	right: 0;
	bottom: 0;
	top: 0;
	right: 0;
	width: 100%;
	height: 100%;
	background-size: 100% 100%;
	background-color: black;
	//background-image: /* our video */;
	background-position: center center;
	background-size: contain;
	object-fit: cover;
// setting fade
.fullpage-wrapper 
	width: 100%!important;
	transform: none!important;
.fp-section 
	width: 100%!important;
	position: absolute;
	left: 0;
	top: 0;
	visibility: hidden;
	opacity: 0;
	//z-index: 0;
	transition: all 1s ease-in-out;
	-webkit-transform: scale(1.1);
	-moz-transform: scale(1.1);
	-ms-transform: scale(1.1);
	-o-transform: scale(1.1);
	transform: scale(1.1);
	transition-delay: 1s;
.fp-section.active 
	visibility: visible;
	opacity: 1;
	//z-index: 1;
	-webkit-transform: scale(1);
	-moz-transform: scale(1);
	-ms-transform: scale(1);
	-o-transform: scale(1);
	transform: scale(1);
